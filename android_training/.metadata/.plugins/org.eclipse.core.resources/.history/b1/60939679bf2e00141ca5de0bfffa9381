package com.test.StringTest;

public class Refer {

    public static void main(String[] args) {

        // String str = "abc";
        String str = new String("abc");

        change(str);

        System.out.println(str);
        
        User user = new User();
        
    }

    public static void change(String str) {
        str = "gbc";
    }

    static class User {
        String name;
    }
}
